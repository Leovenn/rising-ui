    "@rising-ui/play": "workspace:^0.0.0",
    "@rising-ui/rising-ui": "*",
    "@rising-ui/theme-chalk": "workspace:^1.0.0",
    "@rising-ui/utils": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue": "^3.0.0",
