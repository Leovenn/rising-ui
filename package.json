{
  "name": "rising-ui",
  "version": "0.0.0",
  "description": "一个简约的vue3组件库",
  "keywords": [
    "vue3组件库",
    "rising-ui",
    "组件库",
    "component library",
    "vue",
    "vue3"
  ],
  "scripts": {
    "dev": "vite",
    "build:vite": "vue-tsc --noEmit && vite build",
    "build": "node ./build/clean && vite build && node ./build",
    "build:pub": "pnpm run build && node ./build/publish",
    "preview": "vite preview"
  },
  "author": "leoven",
  "license": "MIT",
  "homepage": "https://leovenn.github.io/rising-ui-docs",
  "dependencies": {
    "@rising-ui/components": "workspace:^1.0.0",
    "@rising-ui/constants": "workspace:^1.0.0",
    "@rising-ui/rising-ui": "workspace:^0.0.4",
    "@rising-ui/theme-chalk": "workspace:^1.0.0",
    "@rising-ui/utils": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "^4.6.4",
    "unplugin-vue-macros": "^0.9.2",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4"
  },
  "type": "module",
  "files": [
    "dist"
  ]
